<Window x:Class="Пр_13.Autorization"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Пр_13"
        mc:Ignorable="d"
        Title="Авторизация" Height="188" Width="308" ResizeMode="CanMinimize" Activated="Window_Activated" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel RenderTransformOrigin="0.5,0.5" VerticalAlignment="Center">
            <StackPanel.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.331"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.604"/>
                </TransformGroup>
            </StackPanel.RenderTransform>
            <PasswordBox x:Name="txtPas" PasswordChar="*" FontSize="16" Width="275"></PasswordBox>
            <Button Content="Войти" Height="48" Width="274" FontSize="16" IsDefault="True" Click="Enter_Click"/>
            <Button Content="Отмена" Height="48" Width="274" FontSize="16" IsDefault="True" Click="Esc_Click"/>
        </StackPanel>
    </Grid>
</Window>
